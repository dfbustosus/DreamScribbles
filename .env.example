# DreamScribbles configuration
# Copy this file to `.env` and edit values as needed.

# Optional Hugging Face token (needed for some gated models)
DREAMSCRIBBLES_HF_TOKEN=

# Model IDs (change if you want different bases)
DREAMSCRIBBLES_MODEL_ID=runwayml/stable-diffusion-v1-5
DREAMSCRIBBLES_CONTROLNET_ID=lllyasviel/sd-controlnet-scribble

# Device preference: cuda | mps | cpu (leave empty for auto-detect)
DREAMSCRIBBLES_DEVICE_PREFERENCE=

# Web server
DREAMSCRIBBLES_PORT=7860
DREAMSCRIBBLES_SHARE=false

# Safety
DREAMSCRIBBLES_DISABLE_SAFETY_CHECKER=false
